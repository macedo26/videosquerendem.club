	<script type="text/javascript">
		var getUrlParameter = function getUrlParameter(sParam) {
			var sPageURL = window.location.search.substring(1),
				sURLVariables = sPageURL.split('&'),
				sParameterName,
				i;

			for (i = 0; i < sURLVariables.length; i++) {
				sParameterName = sURLVariables[i].split('=');

				if (sParameterName[0] === sParam) {
					return sParameterName[1] === undefined ? true : decodeURIComponent(sParameterName[1]);
				}
			}
			return false;
		};

		var pixel = getUrlParameter('pixel');
		var src = getUrlParameter('src');
		var d = getUrlParameter('d');

		// d aux
		if (d === false || d === undefined) {
			d = 12.3; // 14 min padrão
		} else {
			d = d; // default value
		}

		setTimeout(() => {
			$('.esconder').fadeIn('slow');
		}, d * 60000);

		fbq('init', getUrlParameter('pixel'));
		fbq('track', 'ViewContent');

		document.getElementById("locationJS").innerHTML = geoplugin_city();

		var a = document.getElementsByTagName('a');

		function addToCart() {
			fbq('init', 'AddToCart');
		}

		for (var i = 0; i < a.length; i++) {
			a[i].href = "https://pay.kiwify.com.br/0lQtqzx?src=" + src;
			a[i].target = "_blank";
			a[i].setAttribute('onclick', 'addToCart()');
		}

		var paramns = location.search;
		var back_redirect_back_link = 'https://pay.kiwify.com.br/0lQtqzx/' + paramns;

		history.pushState({}, "", location.href);
		history.pushState({}, "", location.href);

		window.onpopstate = function () {
			setTimeout(function () {
				location.href = back_redirect_back_link;
			}, 1);
		};
